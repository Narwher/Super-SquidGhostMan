            <string>///Display and cycle thru messages

if messageDraw2 == "" and currentMsg == 1{
    if (gamepad_button_check_pressed(0,gp_face1)){
        mouse_clear(mb_left);
        instance_destroy();
        global.gameMove = true;
    }
}else if messageDraw2 != "" and currentMsg == 1{
    if (gamepad_button_check_pressed(0,gp_face1)){
        mouse_clear(mb_left);
        currentMsg +=1;
        global.seeMayor = true;
    }
}else if messageDraw3 == "" and currentMsg == 2{
    if (gamepad_button_check_pressed(0,gp_face1)){
        mouse_clear(mb_left);
        instance_destroy();
        global.gameMove = true;
    }
}else if messageDraw3 != "" and currentMsg == 2{
    if (gamepad_button_check_pressed(0,gp_face1)){
        mouse_clear(mb_left);
        currentMsg +=1;
    }
}else if messageDraw4 == "" and currentMsg == 3{
    if (gamepad_button_check_pressed(0,gp_face1)){
        mouse_clear(mb_left);
        instance_destroy();
        global.gameMove = true;
    }
}else if messageDraw4 != "" and currentMsg == 3{
    if (gamepad_button_check_pressed(0,gp_face1)){
        keyboard_clear(vk_space);
        mouse_clear(mb_left);
        currentMsg +=1;
    }
}

if currentMsg &gt;=4{
    if (gamepad_button_check_pressed(0,gp_face1)){
        global.gameMove = true
        instance_destroy();
    }
}



</string>
