            <string>if isShopping == false &amp;&amp; endShopping == false{
    if keyboard_check_pressed(vk_space) || mouse_check_button_pressed(mb_left){
        keyboard_clear(vk_space);
        mouse_clear(mb_left);
        isShopping = true;
    }
}
if endShopping == true{
    if keyboard_check_pressed(vk_space) || mouse_check_button_pressed(mb_left){
        keyboard_clear(vk_space);
        mouse_clear(mb_left);
        global.gameMove = true;
        instance_destroy();   
    }
}   

if isShopping == true{

if keyboard_check_pressed(ord('D')) || keyboard_check_pressed(vk_right){
    if currentOption &lt; 3{
        currentOption = currentOption + 1;
        sound_play(snd_click)
    }
}else if keyboard_check_pressed(ord('A')) || keyboard_check_pressed(vk_left){
    if currentOption &gt; 1 {
        currentOption = currentOption - 1;
        sound_play(snd_click)
    }
}

if currentOption == 1{ //ammo for bubble cannon
    if keyboard_check_pressed(vk_return){
        keyboard_clear(vk_return);
        if global.money &gt;=15{
            global.money -= 15;
            global.ammo += 250;
            msgEnd = "Thanksss." 
            sound_play(snd_cash) 
            isShopping = false;
            endShopping = true; 
        }else{
            msgEnd = "You don't have enough CLAMsss..."  
            isShopping = false;
            endShopping = true; 
        } 
    }
    msg = "250 shots for your BUBBLE CANNON."
    cost = "15 CLAMs"
}else if currentOption == 2{ //rockets
    if keyboard_check_pressed(vk_return){
        keyboard_clear(vk_return);
        if global.hasRocket == true{
            if global.money &gt;=20{
                global.money -= 20;
                global.missile += 20;
                msgEnd = "Thanksss." 
                sound_play(snd_cash) 
                isShopping = false;
                endShopping = true; 
            }else{
                msgEnd = "You don't have enough CLAMsss..."  
                isShopping = false;
                endShopping = true;  
            }
        }else{
            msgEnd = "You don't have a rocket LAUNCHER..."  
            isShopping = false;
            endShopping = true; 
        }    
    }
    msg = "20 ROCKETss for a rocket LAUNCHER."
    cost = "20 CLAMs"
}else if currentOption == 3{ //oldass key
    if keyboard_check_pressed(vk_return){
        keyboard_clear(vk_return);
        if global.oldasskey == false{
            if global.money &gt;=100{
                global.money -= 100;
                global.oldasskey = true;
                msgEnd = "...ssucker."  
                sound_play(snd_cash)
                isShopping = false;
                endShopping = true; 
            }else{
                msgEnd = "You don't have enough CLAMsss..."  
                isShopping = false;
                endShopping = true; 
            }
        }else{
            msgEnd = "This item is SOLD OUT."  
            isShopping = false;
            endShopping = true; 
        }    
    }
    msg = "Thiss is an OLD ASS KEY. #I don't recall what it'ss for."
    cost = "100 CLAMs"
}

}
</string>
